       <Good>838</Good>
      </Item>
   </ModOp>
   <ModOp Type='add' GUID='29' Path='/Values/Trader/Progression/EndGame/GoodsProduction'
     Condition="!/Values/Trader/Progression/EndGame/GoodsProduction/Item[Good='838']">
