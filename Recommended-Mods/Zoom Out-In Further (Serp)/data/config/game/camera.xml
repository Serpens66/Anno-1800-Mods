    <Preset ID="-3" Height="1" Pitch="0.875" MinPitch="-0.375" MaxPitch="1.40" Fov="0.70" ExtendedZoom="false" EdgeScrollingSpeed="8.0f" GamepadPanningSpeed="6.0f"/>/>
    <Preset ID="-2" Height="6" Pitch="0.875" MinPitch="-0.375" MaxPitch="1.40" Fov="0.68" ExtendedZoom="false" EdgeScrollingSpeed="8.0f" GamepadPanningSpeed="6.0f"/>/>

