	</ModOp>
    
  <ModOp Type="merge" Path="Normal/Settings">
    <Settings 
      MaxZoomPreset="24">
    </Settings>
  </ModOp>
  <ModOp Type="merge" Path="Normal/Settings">
    <Settings 
      MinZoomPreset="-3">
    </Settings>
  </ModOp>
  
  <ModOp Type="merge" Path="Normal/Settings">
    <Settings 
      MinFromHeight="1.0">
    </Settings>
  </ModOp>
  <ModOp Type="merge" Path="Normal/Settings">
    <Settings 
      MinTerrainDistance="1.0">
    </Settings>
  </ModOp>

</ModOps> 

