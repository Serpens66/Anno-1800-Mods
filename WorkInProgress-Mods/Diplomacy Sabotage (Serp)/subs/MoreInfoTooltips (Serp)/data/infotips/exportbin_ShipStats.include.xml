  <!-- 500460 StrategicViewShip (wenn man im traderoutemenü über schiff hovered) -->
  <!-- UND auch wenn man in Schiffsleiste über Schiffe hovered! -->
  <!-- Show Cargo/Sockets also for allied/spied on -->
  <ModOp Type="add" Path="//InfoTipData[Guid='500460']/InfoElement[InfoElement/Text/TextGUID='9986']/VisibilityElement[VisibilityElement/Condition='[MetaObjects SessionGameObject([RefOid]) IsOwnedByCurrentParticipant]']">
    <!-- we are owner  or allied -->
    <VisibilityElement>
      <ElementType>
        <ElementType>1</ElementType>
      </ElementType>
      <CompareOperator />
      <ResultType>
        <ResultType>1</ResultType>
      </ResultType>
      <ExpectedValueInt>3</ExpectedValueInt>
      <Condition>[Participants TopLevelDiplomacyStateTo([MetaObjects SessionGameObject([RefOid]) Owner], [Participants GetCurrentParticipantID()])]</Condition>
    </VisibilityElement>
    <!-- or we spied on him -->
    <VisibilityElement>
      <ElementType>
        <ElementType>1</ElementType>
      </ElementType>
      <CompareOperator>
        <CompareOperator>5</CompareOperator>
      </CompareOperator>
      <ResultType>
        <ResultType>1</ResultType>
      </ResultType>
      <Condition>[Economy MetaStorage StorageAmount([AssetForValue(1500006094, [MetaObjects SessionGameObject([RefOid]) Owner]) Guid])]</Condition>
    </VisibilityElement>
  </ModOp>
