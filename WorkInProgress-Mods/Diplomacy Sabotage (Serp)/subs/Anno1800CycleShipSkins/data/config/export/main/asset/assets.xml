<ModOps>
  <ModOp Type="add" GUID='2001271' Path="/Values/GUIConfig/KeyBindings">
    <!-- CycleSkins -->
    <Item>
      <KeyTypes>Shift;Alt;V</KeyTypes>
      <Text>1600000046</Text>
      <Command>for k,v in pairs(session.selection) do ts.Objects.GetObject(tonumber(string.match(v:getName(), "oid (.*)"))).SetCycleSkins(1) end</Command>
      <Active>Session;WorldMap;Menu;Pause;Popup</Active>
      <AvailableOnPlatforms>PC;Xbox;PS;Stadia</AvailableOnPlatforms>
      <AudioEvent>200634</AudioEvent>
      <Configurable>1</Configurable>
      <HideInOptionMenu>0</HideInOptionMenu>
      <AllowMultipleShortcuts>1</AllowMultipleShortcuts>
    </Item>
  </ModOp>
</ModOps>