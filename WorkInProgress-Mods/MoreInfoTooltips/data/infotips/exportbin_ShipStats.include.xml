  <!-- https://github.com/jakobharder/anno1800-mod-loader/blob/main/doc/modloader11.md#patching-infotip-exportbin -->
    <!-- to export the tooltips from the game use FileDBReader and the FileFormat "infotip.xml" and unpack "data\infotips\export.bin" -->
    <!-- or download it in anno modding discord here: -->
    <!-- https://discord.com/channels/578443209940926465/1146098268850376816/1149910673678159964 -->

  <!-- All code here is very compatible to other mods, since it just adds more, not change anything -->


    <!-- unfortunately there is no ToolOneHelper/StaticData for attack Range, otherwise I would display this too for the ships in shipyard.. -->
    <!-- not even for the ship object itself range is displayable as number -.- -->

  <!-- Also StackLimit can not be displayed in Shipyard -->

  
  <!-- add the item name to the socketed items list -->
  <ModOp Type="add" Path="//InfoElement[Icon/IconText='[MetaObjects SessionGameObject([RefOid]) ItemContainer Sockets AT(#index0#) Icon]']/Text[not(Text) and not(TextGUID)]">
    <Text>[MetaObjects SessionGameObject([RefOid]) ItemContainer Sockets AT(#index0#) Text]</Text>
  </ModOp>
  <ModOp Type="add" Path="//InfoElement[Icon/IconText='[Selection Picked ItemContainer Sockets AT(#index0#) Icon]']/Text[not(Text) and not(TextGUID)]">
    <Text>[Selection Picked ItemContainer Sockets AT(#index0#) Text]</Text>
  </ModOp>
  
  <!-- only show Cargo if the SlotAmount is > 0 (it is 0 for UnlimitedSlotCount and we dont want to show Cargo for Unlimited, because it may be too much to display) -->
  <ModOp Type="AddNextSibling" Path="//InfoElement[VisibilityElement/VisibilityElement/Condition='[MetaObjects SessionGameObject([RefOid]) ItemContainer Cargo COUNT]' and InfoElement/InfoElement/Text/Text='[MetaObjects SessionGameObject([RefOid]) ItemContainer Cargo AT(#index0#) Value]']/VisibilityElement/VisibilityElement">
    <VisibilityElement>
      <ElementType>
        <ElementType>1</ElementType>
      </ElementType>
      <CompareOperator>
        <CompareOperator>1</CompareOperator>
      </CompareOperator>
      <ResultType>
        <ResultType>1</ResultType>
      </ResultType>
      <ExpectedValueInt>0</ExpectedValueInt>
      <Condition>[ToolOneHelper SlotAmount([RefGuid])]</Condition>
    </VisibilityElement>
  </ModOp>
  <ModOp Type="AddNextSibling" Path="//InfoElement[VisibilityElement/VisibilityElement/Condition='[Selection Picked ItemContainer Cargo COUNT]' and InfoElement/InfoElement/Text/Text='[Selection Picked ItemContainer Cargo AT(#index0#) Value]']/VisibilityElement/VisibilityElement">
    <VisibilityElement>
      <ElementType>
        <ElementType>1</ElementType>
      </ElementType>
      <CompareOperator>
        <CompareOperator>1</CompareOperator>
      </CompareOperator>
      <ResultType>
        <ResultType>1</ResultType>
      </ResultType>
      <ExpectedValueInt>0</ExpectedValueInt>
      <Condition>[ToolOneHelper SlotAmount([Selection Picked GUID])]</Condition>
    </VisibilityElement>
  </ModOp>
  
