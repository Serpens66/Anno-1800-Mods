  <!-- AreaTolerance -->
  <!-- SpecialistSpawnProbabilityIfPercent -->
  <!-- ResolverUnitDecreaseRateInPercent (except 21528 because its hardcoded, means not in infotips, so we can not ad condition) -->
  <!-- SpecialUnitHappinessThresholdUpgrade -->
  
