  <ModOp Type="replace" GUID='142755' Path="/Values/DifficultySetup" Content="@141149/DifficultySetup"/>
  <ModOp Type="replace" GUID='142756' Path="/Values/DifficultySetup" Content="@141150/DifficultySetup"/>
